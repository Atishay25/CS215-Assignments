rng(1);

CovMatrix = [1.6250 -1.9486;-1.9486 3.8750];
mu = [1; 2];

[EigenVector,EigenVal] = eig(CovMatrix);
P = realsqrt(EigenVal);
A = EigenVector*P;
disp(P);
disp(EigenVector);
disp(Ei)


for N = [10 100 1000 10000 100000]
    K = randn(2,N); 
    points = zeros(2,N);
    for i = 1:N
        points(:,i) = mu + A*K(:,i);
    end
    figure(i);
    scatter(points(1,:),points(2,:));
end
        